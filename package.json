{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rimraf lib/ && babel ./src --out-dir lib/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "build-watch": "rimraf lib/ && babel ./src --watch --out-dir lib/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node ./bin/www",
    "start-dev": "nodemon ./bin/www"
  },
  "dependencies": {
    "await-to-js": "^2.0.1",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "crypto-promise": "^2.1.0",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "~4.15.5",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "mysql2": "^1.5.1",
    "parse-error": "^0.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "rimraf": "^2.6.3",
    "sequelize": "^4.31.2",
    "sequelize-cli": "^3.0.0",
    "serve-favicon": "~2.4.5",
    "unirest": "^0.6.0",
    "validator": "^9.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.19.1"
  }
}
